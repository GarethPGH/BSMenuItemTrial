@model BSMenuItemManager

@{
  ViewBag.Title = "Data Driven Navbar";
}

<nav class="navbar navbar-default navbar-static-top"
     role="navigation">
    <div class="container">
        <ul class="nav navbar-nav">
            @foreach (BSMenuItem item in Model.Menus)
            {
            <li>
                <a href="@item.Action">
                    @item.Title
                </a>
            </li>
            }
        </ul>
    </div>
</nav>